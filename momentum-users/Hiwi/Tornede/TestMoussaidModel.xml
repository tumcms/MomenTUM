			<lattice id="0" latticeType="Quadratic" neigborhoodType="Touching" fillingType="ScenarioLayout"  cellEdgeSize="0.46"/>
	        <graph id="0" type="Visibility">
        		<property name="cornerDistance" type="Double" value="2"/>
        	</graph>
		</scenario>
	</layouts>
</simulator>