			</obstacle>			
			<obstacle name="outerWallTop" type="Wall">
				<point x="0.0" y="0.0"/>
				<point x="100.0" y="0.0"/>
			</obstacle>			
			<obstacle name="outerWallRight" type="Wall">
				<point x="100.0" y="0.0"/>
