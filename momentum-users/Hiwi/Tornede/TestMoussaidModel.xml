				<point x="6.0" y="0.0"/>
				<point x="6.0" y="10.0"/>
				<point x="0.0" y="10.0"/>
			</area>
			<area id="1" name="dest" type="Destination">
				<point x="98.0" y="0.0"/>
				<point x="100.0" y="0.0"/>
				<point x="100.0" y="10.0"/>
				<point x="98.0" y="10.0"/>
			</area>
			<obstacle name="spike1" type="Solid">
				<point x="65.0" y="0.0"/>
				<point x="75.0" y="0.0"/>
